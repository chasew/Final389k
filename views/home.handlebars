<h2>Featured Restaurants:</h2>
<link rel="stylesheet" href="https://unpkg.com/flickity@2/dist/flickity.min.css" media="screen" />
<script src="https://unpkg.com/flickity@2/dist/flickity.pkgd.min.js"></script>
<!-- Flickity HTML init -->
<div class="carousel" data-flickity='{ "imagesLoaded": true, "percentPosition": false }'>
    <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/3/36/McDonald%27s_Golden_Arches.svg/300px-McDonald%27s_Golden_Arches.svg.png"
         alt="mcdonalds"/>
    <img src="data:image/png;base64,"
         alt'starbucks'/>
    <img src="data:image/jpeg;base64,"
         alt="vigilante"/>
    <img src="data:image/png;base64,"
         alt="chipolte" />
    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRYWdFxLE4tTRmOz_1iSLle5ZYhz4KI05fzcRhcwGC4SMSSH_fAOg&s"
         alt="marathon deli"/>

</div>



<script>
    $('.carousel').flickity({
        cellAlign: 'left',
        contain: true
    });
</script>



{{#if tag}}
<h2>Recent Posts in {{tag}}:</h2>
{{else}}
<h2>Recent Posts:</h2>
{{/if}}



{{#each data}}
<article>
    <h2 class="title">{{this.restaurant_name}}</h2> <i>{{date}}</i>
    <hr>
    <div class="content">
        {{{this.content}}}...
    </div>
    <a href="/post/{{this.slug}}">Read more...</a>
</article>
{{else}}



<article>
    <h1>There are no blog posts yet. :(</h1>
    <a href="/create">Write your first post!</a>
</article>
{{/each}}
